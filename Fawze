    local root = char.HumanoidRootPart
    local bv = Instance.new("BodyVelocity")
    bv.Velocity = Vector3.new(0, 0, 0)
    bv.MaxForce = Vector3.new(40000, 40000, 40000)
    bv.Parent = root
    
    local target = savedLocation + Vector3.new(0, 2, 0)
    
    local connection
    connection = RunService.Heartbeat:Connect(function()
        if not bv or not bv.Parent then
            connection:Disconnect()
            return
        end
        
        local direction = (target - root.Position).Unit
        local distance = (target - root.Position).Magnitude
        
        if distance > 3 then
            bv.Velocity = direction * 18
        else
            bv:Destroy()
            connection:Disconnect()
            showNotification("Ø·ÙŠØ±Ø§Ù† Ù„Ù„Ù…ÙˆÙ‚Ø¹", "ÙˆØµÙ„Øª Ù„Ù„Ù…ÙˆÙ‚Ø¹!")
        end
    end)
