-- ===== Ù…ÙŠØ²Ø© Ø§Ù„Ø·ÙŠØ±Ø§Ù† Ø§Ù„Ø¹Ø§Ø¯ÙŠ =====
btnFlot.MouseButton1Click:Connect(function()
    State.flotOn = not State.flotOn
    
    if State.flotOn then
        local char = LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            if State.flotBV then
                State.flotBV:Destroy()
            end
            
            local bv = Instance.new("BodyVelocity")
            bv.Velocity = Vector3.new(0, 0, 0)
            bv.MaxForce = Vector3.new(40000, 40000, 40000)
            bv.Parent = char.HumanoidRootPart
            State.flotBV = bv
            
            local connection
            connection = RunService.Heartbeat:Connect(function()
                if not State.flotOn or not char or not char.Parent then
                    connection:Disconnect()
                    return
                end
                
                local root = char:FindFirstChild("HumanoidRootPart")
                local camera = Workspace.CurrentCamera
                
                if root and camera and State.flotBV then
                    local lookVector = camera.CFrame.LookVector
                    State.flotBV.Velocity = lookVector * 50
                end
            end)
            
            showNotification("Ø·ÙŠØ±Ø§Ù†", "Ù…ÙØ¹Ù„ - Ø­Ø±Ùƒ Ø§Ù„ÙƒØ§Ù…ÙŠØ±Ø§ Ù„Ù„ØªØ­ÙƒÙ…")
