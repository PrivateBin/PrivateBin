local function showNotification(title, text)
    pcall(function()
        StarterGui:SetCore("SendNotification", {
            Title = title,
            Text = text,
            Duration = 3
        })
    end)
end

-- ===== Ø§Ù„ØªØ­ÙƒÙ… Ø¨Ø§Ù„ÙˆØ§Ø¬Ù‡Ø© =====
local isMinimized = false

local function toggleMinimize()
    isMinimized = not isMinimized
    if isMinimized then
        btnPlatform.Visible = false
        btnFlot.Visible = false
        btnEBS.Visible = false
        btnSaveLocation.Visible = false
        btnFlyToLocation.Visible = false
        btnAntiSpeed.Visible = false
        mainFrame.Size = UDim2.new(0, CONFIG.uiSize.X, 0, 30)
        minimizeBtn.Text = "+"
    else
        btnPlatform.Visible = true
        btnFlot.Visible = true
        btnEBS.Visible = true
        btnSaveLocation.Visible = true
        btnFlyToLocation.Visible = true
        btnAntiSpeed.Visible = true
        mainFrame.Size = UDim2.new(0, CONFIG.uiSize.X, 0, CONFIG.uiSize.Y)
        minimizeBtn.Text = "-"
    end
end

