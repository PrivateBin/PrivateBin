                for _, child in pairs(player.Character:GetChildren()) do
                    if child:IsA("Highlight") or child:IsA("BillboardGui") then
                        child:Destroy()
                    end
                end
            end
        end
        
        showNotification("EBS", "Ù…ØªÙˆÙ‚Ù")
    end
end)

