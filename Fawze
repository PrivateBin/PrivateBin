-- ===== Ù…ÙŠØ²Ø© Anti-Speed =====
local antiSpeedHoldTime = 0
local antiSpeedHolding = false

btnAntiSpeed.MouseButton1Down:Connect(function()
    antiSpeedHolding = true
    antiSpeedHoldTime = tick()
    
    while antiSpeedHolding do
        wait(0.1)
        if tick() - antiSpeedHoldTime >= 5 then
            -- Ø¹Ø±Ø¶ ÙˆØ§Ø¬Ù‡Ø© Ø§Ù„Ø³Ø±Ø¹Ø§Øª Ø¨Ø¹Ø¯ 5 Ø«ÙˆØ§Ù†ÙŠ
            speedGui.Enabled = true
            antiSpeedHolding = false
            showNotification("Anti-Speed", "Ø§Ø®ØªØ± Ø§Ù„Ø³Ø±Ø¹Ø© Ø§Ù„Ù…Ø·Ù„ÙˆØ¨Ø©")
            break
        end
    end
end)

btnAntiSpeed.MouseButton1Up:Connect(function()
    if tick() - antiSpeedHoldTime < 5 then
        -- ØªØºÙŠÙŠØ± Ø§Ù„Ø³Ø±Ø¹Ø© Ø¨Ø´ÙƒÙ„ Ø¯ÙˆØ±ÙŠ
        State.speedCycleIndex = State.speedCycleIndex + 1
        if State.speedCycleIndex > #State.speedCycles then
            State.speedCycleIndex = 1
        end
        
        State.currentSpeed = State.speedCycles[State.speedCycleIndex]
        
        local char = LocalPlayer.Character
        if char then
            local humanoid = char:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = State.currentSpeed
            end
        end
        
        showNotification("Anti-Speed", "Ø§Ù„Ø³Ø±Ø¹Ø©: " .. State.currentSpeed)
    end
    antiSpeedHolding = false
end)

