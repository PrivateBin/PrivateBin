btnPlatform.MouseButton1Click:Connect(function()
    State.platformOn = not State.platformOn
    
    if State.platformOn then
        local char = LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            if State.platformPart then
                State.platformPart:Destroy()
            end
            
            local platform = Instance.new("Part")
            platform.Size = Vector3.new(8, 1, 8)
            platform.Anchored = true
            platform.CanCollide = true
            platform.BrickColor = BrickColor.new("Bright orange")
            platform.Material = Enum.Material.Neon
            platform.Position = char.HumanoidRootPart.Position + Vector3.new(0, 10, 0)
            platform.Parent = Workspace
            State.platformPart = platform
            
            showNotification("Ù…Ù†ØµØ©", "ØªÙ… ØªÙØ¹ÙŠÙ„ Ø§Ù„Ù…Ù†ØµØ© Ø§Ù„Ø¨Ø±ØªÙ‚Ø§Ù„ÙŠØ© ÙÙŠ Ø§Ù„Ø£Ø¹Ù„Ù‰")
        end
    else
        if State.platformPart then
            State.platformPart:Destroy()
            State.platformPart = nil
        end
        showNotification("Ù…Ù†ØµØ©", "ØªÙ… Ø¥ÙŠÙ‚Ø§Ù Ø§Ù„Ù…Ù†ØµØ©")
    end
